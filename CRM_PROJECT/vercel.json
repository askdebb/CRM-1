{
    "builds": [{
        "src": "CRM_PROJECT/wsgi.py",
        "use": "@vercel/python",
        "config": { 
            "maxLambdaSize": "15mb",
            "runtime": "python3.9"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "CRM_PROJECT/wsgi.py"
        }
    ]
}